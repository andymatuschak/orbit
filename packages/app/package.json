{
  "name": "@withorbit/app",
  "version": "1.0.0",
  "main": "./index.js",
  "scripts": {
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start",
    "ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
    "web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --web -p 19006",
    "android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "analyze:web": "source-map-explorer 'web-build/_expo/static/js/web/*.js' 'web-build/_expo/static/js/web/*.js.map'",
    "build:web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo export -p web --output-dir web-build --source-maps",
    "deploy:web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 scripts/deploy_web.sh",
    "generateBrowserCompatibility": "scripts/generate_browser_compatibility_regex.sh"
  },
  "dependencies": {
    "@op-engineering/op-sqlite": "^2.0.13",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@sentry/react-native": "5.19.1",
    "@withorbit/api-client": "0.0.1",
    "@withorbit/core": "0.0.1",
    "@withorbit/embedded-support": "0.0.1",
    "@withorbit/ingester": "0.0.1",
    "@withorbit/store-fs": "0.0.1",
    "@withorbit/store-shared": "0.0.1",
    "@withorbit/store-web": "0.0.1",
    "@withorbit/ui": "0.0.1",
    "expo": "~50.0.14",
    "expo-application": "~5.8.0",
    "expo-constants": "~15.4.2",
    "expo-crypto": "~12.8.0",
    "expo-device": "~5.9.0",
    "expo-font": "^11.10.0",
    "expo-linking": "~6.2.1",
    "expo-router": "~3.4.0",
    "firebase": "^10.7.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-blob-jsi-helper": "^0.3.1",
    "react-native-keyevent": "^0.3.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-mathjax-svg": "andymatuschak/react-native-mathjax-svg",
    "react-native-quick-base64": "^2.0.8",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6",
    "sentry-expo": "~7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-syntax-import-attributes": "^7.23.3",
    "@expo/metro-runtime": "~3.1.3",
    "@types/react": "~18.2.45",
    "@withorbit/sample-data": "0.0.1",
    "expo-dev-client": "~3.3.4",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.1.3"
  },
