{
  "expo": {
    "name": "orbit",
    "slug": "orbit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets2/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets2/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets2/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets2/favicon.png",
      "build": {
        "babel": {
          "include": ["@withorbit/ui"]
        }
      },
      "bundler": "metro"
    }
  }
}
